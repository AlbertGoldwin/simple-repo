{
  "name": "google-github-oauth2-nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:dev": "nodemon src/index.ts",
    "build": "rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "db:migrate": "npx prisma migrate dev --name 'users' --create-only",
    "db:generate": " npx prisma generate",
    "db:push": "npx prisma db push"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/qs": "^6.9.7",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "pre-commit": "^1.2.2",
    "prisma": "^4.8.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "axios": "^1.2.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "qs": "^6.11.0",
    "zod": "^3.20.2"
  }
}
